   "source": [
    "import cv2\n",
    "\n",
    "image = cv2.imread('Images/input.jpg')\n",
    "\n",
    "smaller = cv2.pyrDown(image)\n",
    "larger = cv2.pyrUp(smaller)\n",
    "\n",
    "cv2.imshow('Original',image)\n",
    "cv2.imshow('Smaller', smaller)\n",
    "cv2.imshow('Larger', larger)\n",
    "cv2.waitKey()\n",
    "cv2.destroyAllWindows()"
   ]
  }
